{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "resourceGroupName": {
        "value": "${AZURE_RESOURCE_GROUP}"
      },
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "aiFoundryResourceName": {
        "value": "${AZURE_AI_FOUNDRY_RESOURCE_NAME}"
      },
      "aiDeploymentsLocation": {
        "value": "${AZURE_LOCATION}"
      },
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "principalType": {
        "value": "${AZURE_PRINCIPAL_TYPE}"
      },
      "aiProjectDeployments": {
        "value": []
      },
      "aiProjectConnections": {
        "value": []
      },
      "aiProjectDependentResources": {
        "value": [
          {
            "resource": "AzureContainerRegistry",
            "connection_name": "acr-connection"
          }
        ]
      },
      "enableCoboAgent": {
        "value": "${ENABLE_COBO_AGENT=true}"
      },
      "agentName": {
        "value": "${AGENT_NAME=calculator-agent}"
      }
    }
}
